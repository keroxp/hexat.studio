{
  "name": "hexat.studio",
  "version": "1.0.0",
  "description": "web application for hexat.studio",
  "author": "Yusuke Sakurai <kerokerokerop@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "next"
  },
  "dependencies": {
    "@material-ui/core": "^1.0.0",
    "@material-ui/icons": "^1.0.0",
    "@zeit/next-typescript": "^1.0.0",
    "material-ui": "^1.0.0-beta.47",
    "next": "^6.0.3",
    "normalize.css": "^8.0.0",
    "octicons": "^7.3.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-jsx": "^2.2.6",
    "styled-jsx-plugin-stylus": "^0.0.4",
    "stylus": "^0.54.5",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "@types/next": "^2.4.10",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "@types/styled-jsx": "^2.2.3"
  },
  "babel": {
    "presets": [
      "next/babel",
      "@zeit/next-typescript/babel"
    ],
    "plugins": [
      [
        "styled-jsx/babel",
        {
          "plugins": [
            "styled-jsx-plugin-stylus"
          ]
        }
      ]
    ]
  },
  "typescript": {
    "compileOnSave": false,
    "compilerOptions": {
      "target": "esnext",
      "module": "esnext",
      "jsx": "preserve",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "removeComments": false,
      "preserveConstEnums": true,
      "sourceMap": true,
      "skipLibCheck": true,
      "baseUrl": ".",
      "lib": [
        "dom",
        "es2016"
      ]
    }
  }
}
